  "cli": {
    "defaultCollection": "@nrwl/next"
  },
  "generators": {
    "@nwrl/node": {
      "library": {
        "pascalCaseFiles": true,
        "importPath": "@atlascode/"
      }
    },
    "@nrwl/react": {
      "application": {
        "babel": true
      },
      "library": {
        "style": "none",
        "pascalCaseFiles": true,
        "publishable": false,
        "component": true,
        "buildable": false,
        "importPath": "@atlascode/frontend-"
      },
      "component": {
        "style": "none",
        "export": true,
        "pascalCaseDirectory": true,
        "pascalCaseFiles": true,
        "skipTests": true
      }
    },
    "@nrwl/next": {
      "application": {
        "style": "css",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "frontend"
